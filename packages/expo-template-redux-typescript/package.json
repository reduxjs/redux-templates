{
  "name": "expo-template-redux-typescript",
  "version": "1.0.0",
  "description": "The official Redux+TS template for Expo",
  "main": "index.ts",
  "scripts": {
    "android": "expo start --android",
    "build": "expo prebuild -p android && react-native bundle --entry-file index.ts --bundle-output build/bundle.js --platform android",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "ios": "expo start --ios",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "start": "expo start",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "web": "expo start --web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reduxjs/redux-templates.git",
    "directory": "packages/expo-template-redux-typescript"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "expo": "~52.0.28",
    "expo-status-bar": "~2.0.1",
    "react": "^18.3.1",
    "react-native": "0.76.6",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@react-native-community/cli": "^15.1.3",
    "@react-native-community/cli-platform-android": "^15.1.3",
    "@react-native-community/cli-platform-ios": "^15.1.3",
    "@react-native/eslint-config": "^0.76.6",
    "@testing-library/react-native": "^12.9.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.18",
    "@types/react-test-renderer": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.3",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  }
}
