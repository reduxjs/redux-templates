{
  "name": "expo-template-redux-typescript",
  "version": "1.0.0",
  "description": "The official Redux+TS template for Expo",
  "main": "index.ts",
  "scripts": {
    "android": "expo start --android",
    "build": "expo prebuild -p android && react-native bundle --entry-file index.ts --bundle-output build/bundle.js --platform android --assets-dest build/assets",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "ios": "expo start --ios",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "start": "expo start",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "web": "expo start --web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reduxjs/redux-templates.git",
    "directory": "packages/expo-template-redux-typescript"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
    "expo": "~52.0.37",
    "expo-status-bar": "~2.0.1",
    "react": "^19.0.0",
    "react-native": "0.78.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@eslint/js": "^9.22.0",
    "@react-native-community/cli": "^17.0.0",
    "@react-native-community/cli-platform-android": "^17.0.0",
    "@react-native-community/cli-platform-ios": "^17.0.0",
    "@react-native/eslint-config": "^0.78.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.5",
    "prettier": "^3.5.3",
    "react-test-renderer": "^19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
